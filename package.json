		"element-plus": "^2.2.28",
		"font-awesome": "^4.7.0",
		"mitt": "^3.0.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.0.30",
		"pinia-plugin-persistedstate": "^3.0.2",
